<h2>Login as Normal User</h2>

<%= form_with model: @user, url: login_normal_user_path, local: true do |f| %>
  <%= f.email_field :email, autofocus: true, placeholder: 'Normal User Email' %>
  <%= f.password_field :password, placeholder: 'Password' %>

  <div class="actions">
    <%= f.submit 'Login as Normal User' %>
  </div>
<% end %>
